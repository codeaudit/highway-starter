{
  "name": "highway",
  "version": "0.0.1",
  "file": "models/highway.pb",
  "options": {

    "datasets": {
      "fomoro/mnist": "0.0.1"
    },

    "epochs": 2000,
    "checkpoint_interval": 100,
    "batch_size": 50,

    "train_feed_dict": {
      "x": "<-x",
      "y_": "<-y"
    },
    "validation_feed_dict": {
      "x": "<-x",
      "y_": "<-y"
    },
    "test_feed_dict": {
      "x": "<-x",
      "y_": "<-y",
    },

    "train_ops": { "train<-": "train/train_step" },
    "validation_ops": { "cost<-": "test/accuracy" },
    "test_ops": { "cost<-": "test/accuracy" }
  }
}
